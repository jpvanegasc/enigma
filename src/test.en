# Unary addition calculator

initial: 0 1 1 0 1 1 1
start: q0

state q0:
    read 0 -> null, move_right, goto q0
    read 1 -> null, move_right, goto q1

state q1:
    read 0 -> write 1, move_left, goto q2
    read 1 -> null, move_right, goto q1

state q2:
    read 0 -> null, move_right, goto q3
    read 1 -> null, move_left, goto q2

state q3:
    read 0 -> null, null, null
    read 1 -> write 0, null, goto halt
